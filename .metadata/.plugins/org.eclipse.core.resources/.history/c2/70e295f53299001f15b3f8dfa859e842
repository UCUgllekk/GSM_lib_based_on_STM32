#include "gsm_module.hpp"

int main() {
    // Parameters parameters = load_from_config(...);
    // parameters.PIN = '6';

    GSM_Module gsm(parameters);
    int res = gsm.send_sms("Hello, World!");

    if (res != 0) {
        std::cout << "Error sending SMS: " << res << std::endl;
    }

    if (call()) {
        gsm.make_call();
    }

    return 0;
}
